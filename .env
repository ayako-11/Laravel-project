APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
########################################################################################################################
## Environment variables related to Docker compose                                                                    ##
########################################################################################################################

# COMPOSE_FILE
# https://docs.docker.jp/compose/reference/envvars.html
COMPOSE_FILE=docker-compose-local.yml

# APP_SERVICE
# Must match the service name in docker-compose.yml
# The default value in Laravel Sail is "laravel.test"
APP_SERVICE=laravel

WWWGROUP=1000
WWWUSER=1000

# Domain name when accessing in the local environment.
# It is also necessary to configure the terminal side so that the name can be resolved,
# such as by writing it in the "hosts" file.
# Local環境でアクセスする際のドメイン名。hostsファイルに記載する等、名前解決できるよう端末側の設定も必要。
APP_DOMAIN_NAME=asugiyama.balocco.info

# Configure settings to avoid port number conflicts according to each environment.
# 各環境に合わせてポート番号が競合しないように設定を行う
VITE_PORT=15175
FORWARD_DB_PORT=15435
FORWARD_REDIS_PORT=16382
FORWARD_MAILPIT_PORT=11027
FORWARD_MAILPIT_DASHBOARD_PORT=18027

# database settings
DC_DB_DATABASE=laravel
DC_DB_USERNAME=sail
DC_DB_PASSWORD=secret
